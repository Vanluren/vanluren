{
  "default": {
    "entry": "tab",
    "layout": [
      "cd ~/Developer/Cozero/core/",
      [
        [
          "cd $HOME/Developer/cozero/core/apps/dashboard && yarn run dev",
          [
            "cd $HOME/Developer/cozero/core/apps/api-gateway && yarn run start:dev",
            "cd $HOME/Developer/cozero/core/apps/central-service && yarn run start:dev"
          ]
        ]
      ],
      [
        [
          [
            "cd $HOME/Developer/cozero/core/apps/log-service && yarn run start:dev",
            "cd $HOME/Developer/cozero/core/apps/act-service && yarn run start:dev"
          ],
          [
            "cd $HOME/Developer/cozero/core/apps/share-service && yarn run start:dev",
            "cd $HOME/Developer/cozero/core/apps/auth-service && yarn run start:dev"
          ]
        ]
      ]
    ]
  },
  "website": {
    "entry": "tab",
    "layout": [
      "cd ~/Developer/Cozero/website/",
      [
        [
          "cd $HOME/Developer/Cozero/website/apps/frontend/ && yarn dev",
          "cd $HOME/Developer/Cozero/website/apps/cms/ && yar dev"
        ]
      ]
    ]
  }

}
